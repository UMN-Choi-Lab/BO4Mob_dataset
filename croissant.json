  "@type": "sc:Dataset",
  "name": "BO4Mob_dataset",
  "description": "This dataset contains five traffic networks modeled in SUMO, each accompanied by vehicle link flow measurements derived from PeMS sensor data. Every network includes its own configuration files (network, OD, TAZ, and additional files), along with a corresponding routes.csv file that defines vehicle paths between TAZs. Two versions of the routes are provided: one containing a single route per TAZ pair (single) and another with multiple routes per TAZ pair (multiple). Sensor data indicates the number of vehicles passing through detectors located on specific network links. For each network, sensor data is available for a particular date and time period.",
  "conformsTo": "http://mlcommons.org/croissant/1.0",
  "citeAs": "@article{ryu2025bo4mob, title={BO4Mob: Bayesian Optimization Benchmarks for High-Dimensional Urban Mobility Problem}, author={Seunghee Ryu and Donghoon Kwon and Seongjin Choi and Aryan Deshwal and Seungmo Kang and Carolina Osorio}, year={2025},",
  "url": "https://github.com/UMN-Choi-Lab/BO4Mob_dataset",
  "distribution": [
    {
      "@type": "cr:FileObject",
      "@id": "github-repository",
      "name": "github-repository",
      "description": "BO benchmark on GitHub",
      "contentUrl": "https://github.com/UMN-Choi-Lab/BO4Mob_dataset",
      "encodingFormat": "git+https",
      "sha256": "main"
    },
    {
      "@type": "cr:FileSet",
      "@id": "xml-files",
      "name": "xml-files",
      "description": "Each subfolder under 'network/' contains SUMO XML files for one specific traffic network.",
      "containedIn": {
        "@id": "github-repository"
      },
      "encodingFormat": "application/xml",
      "includes": "network/network_*/{additional.xml,net.xml,od.xml,taz.xml}"
    },
    {
      "@type": "cr:FileSet",
      "@id": "csv-routes-files",
      "name": "csv-routes-files",
      "description": "Each subfolder under 'network/' contains routes CSV files for one specific traffic network. Single route files has only one pair from TAZ to TAZ, while multiple route files has multiple pairs from TAZ to TAZ.",
      "containedIn": {
        "@id": "github-repository"
      },
      "encodingFormat": "text/csv",
      "includes": "network/network_*/{routes_single.csv, routes_multiple.csv}"
    },
    {
      "@type": "cr:FileSet",
      "@id": "csv-sensor-files",
      "name": "csv-sensor-files",
      "description": "Sensor ground-truth vehicle link flow data for each network, stored as edge-wise CSV files.",
      "containedIn": {
        "@id": "github-repository"
      },
      "encodingFormat": "text/csv",
      "includes": "sensor_data/2210*/gt_link_data_*.csv"
    }
  ],
  "recordSets": [
    {
      "@type": "cr:RecordSet",
      "@id": "xml",
      "name": "xml",
      "description": "Metadata about SUMO network-related XML files (net, taz, additional).",
      "field": [
        {
          "@type": "cr:Field",
          "@id": "xml/filename",
          "name": "filename",
          "description": "Name of the XML file (e.g., net.xml, taz.xml).",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "xml-files"
            },
            "extract": {
              "fileProperty": "filename"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "xml/network_name",
          "name": "network_name",
          "description": "Traffic network name inferred from folder structure.",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "xml-files"
            },
            "extract": {
              "fileProperty": "filename"
            },
            "transforms": [
              {
                "regex": "network/(network_.*?)/.*"
              }
            ]
          }
        },
        {
          "@type": "cr:Field",
          "@id": "xml/type",
          "name": "xml_type",
          "description": "Type of XML file (net, taz, additional, rou) inferred from filename.",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "xml-files"
            },
            "extract": {
              "fileProperty": "filename"
            },
            "transforms": [
              {
                "regex": "(additional|net|od|taz)\\.xml"
              }
            ]
          }
        }
      ]
    },
    {
      "@type": "cr:RecordSet",
      "@id": "csv_routes_single",
      "name": "csv_routes_single",
      "description": "Vehicle route definitions per traffic network.",
      "field": [
        {
          "@type": "cr:Field",
          "@id": "csv_routes_single/fromTaz",
          "name": "fromTaz",
          "description": "Origin TAZ ID.",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "csv-routes-files"
            },
            "extract": {
              "column": "fromTaz"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "csv_routes_single/toTaz",
          "name": "toTaz",
          "description": "Destination TAZ ID.",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "csv-routes-files"
            },
            "extract": {
              "column": "toTaz"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "csv_routes_single/route_edges",
          "name": "route_edges",
          "description": "Route edges assigned to the vehicle.",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "csv-routes-files"
            },
            "extract": {
              "column": "route_edges"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "csv_routes_single/start_edge",
          "name": "start_edge",
          "description": "Starting edge of the route.",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "csv-routes-files"
            },
            "extract": {
              "column": "start_edge"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "csv_routes_single/last_edge",
          "name": "last_edge",
          "description": "Ending edge of the route.",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "csv-routes-files"
            },
            "extract": {
              "column": "last_edge"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "csv_routes_single/network_name",
          "name": "network_name",
          "description": "Network name inferred from folder path.",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "csv-routes-files"
            },
            "extract": {
              "fileProperty": "filename"
            },
            "transforms": [
              {
                "regex": "network_(.*?)/"
              }
            ]
          }
        }
      ]
    },
    {
      "@type": "cr:RecordSet",
      "@id": "csv_routes_multiple",
      "name": "csv_routes_multiple",
      "description": "Multiple route definitions with ratios for each TAZ pair in each network.",
      "field": [
        {
          "@type": "cr:Field",
          "@id": "csv_routes_multiple/fromTaz",
          "name": "fromTaz",
          "description": "Origin TAZ ID.",
