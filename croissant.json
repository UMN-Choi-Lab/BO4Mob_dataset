            "fileSet": {
              "@id": "csv-sensor-files"
            },
            "extract": {
              "column": "interval_nVehContrib"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "csv_sensor/network_name",
          "name": "network_name",
          "description": "Network name inferred from the filename.",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "csv-sensor-files"
            },
            "extract": {
              "fileProperty": "filename"
            },
            "transforms": [
              {
                "regex": "gt_link_data_(.*?)_\\d{6}_\\d{2}-\\d{2}\\.csv"
              }
            ]
          }
        }
      ]
    }
  ]
