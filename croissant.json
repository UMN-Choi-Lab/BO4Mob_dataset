            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "csv_routes_multiple/toTaz",
          "name": "toTaz",
          "description": "Destination TAZ ID.",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "csv-routes-files"
            },
            "extract": {
              "column": "toTaz"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "csv_routes_multiple/route_idx",
          "name": "route_idx",
          "description": "Index of the route within same TAZ pair.",
          "dataType": "sc:Integer",
          "source": {
            "fileSet": {
              "@id": "csv-routes-files"
            },
            "extract": {
              "column": "route_idx"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "csv_routes_multiple/ratio",
          "name": "ratio",
          "description": "Ratio of the route within same TAZ pair.",
          "dataType": "sc:Float",
          "source": {
            "fileSet": {
              "@id": "csv-routes-files"
            },
            "extract": {
              "column": "ratio"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "csv_routes_multiple/route_edges",
          "name": "route_edges",
          "description": "Route edges assigned to the vehicle.",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "csv-routes-files"
            },
            "extract": {
              "column": "route_edges"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "csv_routes_multiple/start_edge",
          "name": "start_edge",
          "description": "Starting edge of the route.",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
