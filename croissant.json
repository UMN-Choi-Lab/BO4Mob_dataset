        },
        {
          "@type": "cr:Field",
          "@id": "csv_routes_multiple/last_edge",
          "name": "last_edge",
          "description": "Ending edge of the route.",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "csv-routes-files"
            },
