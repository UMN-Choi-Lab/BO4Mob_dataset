        },
        {
          "@type": "cr:Field",
          "@id": "csv_routes_multiple/network_name",
          "name": "network_name",
          "description": "Network name inferred from folder path.",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "csv-routes-files"
            },
            "extract": {
              "fileProperty": "filename"
            },
